{
    "compilerOptions": {
        "lib": [
            "ESNext"
        ],
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "target": "ESNext",
        "importHelpers": true,
        "resolveJsonModule": true,
        "module": "ESNext",
        "moduleResolution": "Node",
        "skipLibCheck": true
    },
    "include": [
        "**/*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}
